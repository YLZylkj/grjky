<script>
    var ylzbigdataSessionFinish = true;
    var ylzbigdataBtnA = document.getElementById('ylzbigdata_im_btn_a');
    var ylzbigdataPanel = document.getElementById('ylzbigdata_im_panel');
    var ylzbigdataQueryObj
    var ylzbigdataAppConfigResult
    var ylzbigdataPluginBlankDialog = null
    var ylzbigdataPluginNewTab = null
    var ylzbigdataAppConfigScript = document.createElement('script')
    ylzbigdataReady(function() {
        var dataAttr = document.getElementById('ylzbigdata_script').getAttribute('data')
        ylzbigdataQueryObj = JSON.parse(dataAttr)
        ylzbigdataQueryObj.url = ylzbigdataQueryObj.url + '&loadFrom=button'
        if (ylzbigdataBtnA.addEventListener) {
            ylzbigdataBtnA.addEventListener('click', ylzbigdataBtnAClick)
        } else if (ylzbigdataBtnA.attachEvent) {
            ylzbigdataBtnA.attachEvent('onclick', ylzbigdataBtnAClick)
        }
        if (window.addEventListener) {
            window.addEventListener('message', function(e) {
                ylzbigdataWindowMessage(e)
            })
        } else if (window.attachEvent) {
            window.attachEvent('onmessage', function(e) {
                ylzbigdataWindowMessage(e)
            })
        }
        ylzbigdataAppConfigScript.src = 'https://znkf.ylzinfo.com/ylzbigdata_znkf/app/findByAppIdJsonp?appId=' + ylzbigdataQueryObj.appId + '&t=' + new Date().getTime()
        document.body.appendChild(ylzbigdataAppConfigScript)
    })
    function pullDownIm() {
        ylzbigdataBtnA.style.display = 'block'
        ylzbigdataPanel.style.display = 'none'
    }
    function closeIm() {
        ylzbigdataBtnA.style.display = 'block'
        ylzbigdataPanel.style.display = 'none'
        ylzbigdataSessionFinish = true
        document.getElementById('ylzbigdata_im_iframe').src = ''
    }
    function ylzbigdataBtnAClick() {
        if (window.navigator.userAgent.indexOf('MSIE 8.0') > -1) {
            ylzbigdataBtnA.style.display = 'none'
            alert('抱歉，该插件不支持IE8及以下版本，请使用IE8以上版本或其它浏览器')
            return
        }
        if (ylzbigdataAppConfigResult.webPluginBtn.dialogOpenMode === 'blank') {
            if (ylzbigdataPluginBlankDialog === null || ylzbigdataPluginBlankDialog.closed) {
                ylzbigdataPluginBlankDialog = window.open(ylzbigdataQueryObj.url, 'ylzbigdataPluginBlankDialog', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=yes, resizable=yes, left=50, top=50, width=1000, height=600')
            } else {
                ylzbigdataPluginBlankDialog.focus()
            }
        } else if (ylzbigdataAppConfigResult.webPluginBtn.dialogOpenMode === 'newTab') {
            if (ylzbigdataPluginNewTab === null || ylzbigdataPluginNewTab.closed) {
                ylzbigdataPluginNewTab = window.open(ylzbigdataQueryObj.url)
            } else {
                ylzbigdataPluginNewTab.focus()
            }
        } else {
            ylzbigdataBtnA.style.display = 'none'
            ylzbigdataPanel.style.display = 'block'
            if (ylzbigdataSessionFinish) {
                document.getElementById('ylzbigdata_im_iframe').src = ylzbigdataQueryObj.url
            }
            ylzbigdataSessionFinish = false
        }
    }
    function ylzbigdataWindowMessage(e) {
        try {
            var data = JSON.parse(e.data);
            switch (data.method) {
                case 'pullDownIm':
                    pullDownIm();
                    break;
                case 'closeIm':
                    closeIm();
                    break;
                default:
                    break;
            }
        } catch (e) {}
    }
    function ylzbigdataSetStyle(result) {
        // 按钮样式
        // ylzbigdataBtnA.style.width = result.webPluginBtn.linkBtnWidth + 'px'
        document.getElementById('ylzbigdata_im_btn_text').innerHTML = result.webPluginBtn.linkBtnText
        ylzbigdataBtnA.style.backgroundColor = result.webPluginBtn.linkBtnColor || 'rgb(48, 122, 232)'
        ylzbigdataBtnA.style.marginTop = result.webPluginBtn.linkBtnMarginTop + 'px'
        ylzbigdataBtnA.style.marginLeft = result.webPluginBtn.linkBtnMarginLeft + 'px'
        ylzbigdataBtnA.style.marginRight = result.webPluginBtn.linkBtnMarginRight + 'px'
        ylzbigdataBtnA.style.marginBottom = result.webPluginBtn.linkBtnMarginBottom + 'px'
        switch (result.webPluginBtn.linkBtnPosition) {
            case 'lu':
                ylzbigdataBtnA.style.left = 0 + 'px'
                ylzbigdataBtnA.style.top = 0 + 'px'
                break;
            case 'mu':
                ylzbigdataBtnA.style.left = window.innerWidth / 2 + 'px'
                ylzbigdataBtnA.style.top = 0 + 'px'
                break;
            case 'ru':
                ylzbigdataBtnA.style.right = 0 + 'px'
                ylzbigdataBtnA.style.top = 0 + 'px'
                break;
            case 'll':
                ylzbigdataBtnA.style.left = 0 + 'px'
                ylzbigdataBtnA.style.top = window.innerHeight / 2 + 'px'
                break;
            case 'mm':
                ylzbigdataBtnA.style.left = window.innerWidth / 2 + 'px'
                ylzbigdataBtnA.style.top = window.innerHeight / 2 + 'px'
                break;
            case 'rr':
                ylzbigdataBtnA.style.right = 0 + 'px'
                ylzbigdataBtnA.style.top = window.innerHeight / 2 + 'px'
                break;
            case 'ld':
                ylzbigdataBtnA.style.left = 0 + 'px'
                ylzbigdataBtnA.style.bottom = 0 + 'px'
                break;
            case 'md':
                ylzbigdataBtnA.style.left = window.innerWidth / 2 + 'px'
                ylzbigdataBtnA.style.bottom = 0 + 'px'
                break;
            case 'rd':
                ylzbigdataBtnA.style.right = 0 + 'px'
                ylzbigdataBtnA.style.bottom = 0 + 'px'
                break;
            default:
                break;
        }
        // 弹窗样式
        document.getElementById('ylzbigdata_im_panel').style.width = result.webPluginDialog.imDialogWidth + 'px'
        document.getElementById('ylzbigdata_im_panel').style.height = result.webPluginDialog.imDialogHeight + 'px'
        document.getElementById('ylzbigdata_im_panel').style.bottom = result.webPluginDialog.imDialogBottom + 'px'
        document.getElementById('ylzbigdata_im_panel').style.right = result.webPluginDialog.imDialogRight + 'px'
        ylzbigdataBtnA.style.display = 'block'
    }
    function ylzbigdataCallback(resultObj) {
        ylzbigdataAppConfigResult = resultObj
        ylzbigdataSetStyle(resultObj)
        document.body.removeChild(ylzbigdataAppConfigScript)
        ylzbigdataAppConfigScript = null
    }
    function ylzbigdataReady(fn) {
        if (document.addEventListener) {
            document.addEventListener('DOMContentLoaded', function() {
                // 注销事件，避免反复触发
                document.removeEventListener('DOMContentLoaded', arguments.callee, false)
                fn()
            }, false)
        } else if (document.attachEvent) {
            document.attachEvent('onreadystatechange', function() {
                if (document.readyState === 'complete') {
                    document.detachEvent('onreadystatechange', arguments.callee)
                    fn()
                }
            })
        }
    }
</script>


